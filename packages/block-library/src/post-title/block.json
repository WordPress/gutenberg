{
	"apiVersion": 2,
	"name": "core/post-title",
	"category": "design",
	"usesContext": [
		"postId",
		"postType"
	],
	"attributes": {
		"textAlign": {
			"type": "string"
		},
		"level": {
			"type": "number",
			"default": 2
		},
		"isLink": {
			"type": "boolean",
			"default": false
		},
		"rel": {
			"type": "string",
			"attribute": "rel",
			"default": ""
		},
		"linkTarget": {
			"type": "string",
			"default": "_self"
		}
	},
	"supports": {
		"html": false,
		"color": {
			"gradients": true
		},
		"fontSize": true,
		"lineHeight": true,
		"__experimentalSelector": {
			"core/post-title/h1": "h1",
			"core/post-title/h2": "h2",
			"core/post-title/h3": "h3",
			"core/post-title/h4": "h4",
			"core/post-title/h5": "h5",
			"core/post-title/h6": "h6"
		}
	}
}
