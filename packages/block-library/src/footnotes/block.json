{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "core/footnotes",
	"title": "Footnotes",
	"category": "text",
	"description": "",
	"keywords": [ "references" ],
	"textdomain": "default",
	"attributes": {
		"footnotes": {
			"type": "array",
			"default": [],
			"source": "query",
			"selector": "li",
			"query": {
				"content": {
					"type": "string",
					"source": "html",
					"selector": "span"
				},
				"id": {
					"type": "string",
					"source": "attribute",
					"attribute": "id"
				}
			}
		}
	},
	"supports": {
		"html": false
	},
	"style": "wp-block-footnotes"
}
