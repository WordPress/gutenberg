{
  "name": "stylelint-config-wordpress",
  "version": "2.0.0",
  "description": "WordPress shareable config for stylelint",
  "keywords": [
    "css",
    "stylelint",
    "stylelint-config",
    "wordpress",
    "WordPress"
  ],
  "author": "Stylelint",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stylelint/stylelint-config-wordpress.git"
  },
  "homepage": "https://github.com/stylelint/stylelint-config-wordpress",
  "bugs": "https://github.com/stylelint/stylelint-config-wordpress/issues",
  "main": "index.js",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "index.js"
  ],
  "devDependencies": {
    "ava": "^0.11.0",
    "eslint": "^1.10.3",
    "eslint-config-stylelint": "^0.1.0",
    "npmpub": "^3.0.1",
    "stylelint": "^4.1.0"
  },
  "scripts": {
    "ava": "ava --verbose \"__tests__/**/*.js\"",
    "lint": "eslint . --ignore-path .gitignore",
    "pretest": "npm run lint",
    "release": "npmpub",
    "test": "npm run ava"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ]
  }
}
