{
  "name": "stylelint-config-wordpress",
  "version": "1.0.1",
  "description": "WordPress shareable config for stylelint",
  "keywords": [
    "css",
    "stylelint",
    "stylelint-config",
    "wordpress",
    "WordPress"
  ],
  "author": "Stylelint",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stylelint/stylelint-config-wordpress.git"
  },
  "homepage": "https://github.com/stylelint/stylelint-config-wordpress",
  "bugs": "https://github.com/stylelint/stylelint-config-wordpress/issues",
  "main": "index.js",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "index.js"
  ],
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-tape-runner": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-config-stylelint": "^0.1.0",
    "stylelint": "^3.0.2",
    "tape": "^4.2.2"
  },
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "tape": "babel-tape-runner \"__tests__/**/*.js\"",
    "test": "npm run lint && npm run tape"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ]
  }
}
