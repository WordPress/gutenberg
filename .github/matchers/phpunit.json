{
	"problemMatcher": [
		{
			"owner": "phpunit",
			"pattern": [
				{
					"regexp": "^\\d+\\)\\s.*$"
				},
				{
					"regexp": "^(.*Failed\\sasserting\\sthat.*)$",
					"message": 1
				},
				{
					"regexp": "^\\s*$"
				},
				{
					"regexp": "^(.*):(\\d+)$",
					"file": 1,
					"line": 2
				}
			]
		}
	]
}
